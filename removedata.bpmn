<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
             id="duplicateCartWorkflow"
             targetNamespace="http://example.com/bpmn"
             exporter="Camunda Modeler"
             exporterVersion="4.10.0">

    <process id="duplicateCartProcess" name="Duplicate Cart Workflow" isExecutable="true">
        <startEvent id="startEvent" name="Start"/>
        <task id="checkForDuplicates" name="Check for Duplicates"/>
        <exclusiveGateway id="decisionGateway" name="Is Duplicate?"/>
        <task id="approveCart" name="Approve Cart"/>
        <task id="rejectCart" name="Reject Cart"/>
        <endEvent id="endEvent" name="End"/>

        <sequenceFlow id="flow1" sourceRef="startEvent" targetRef="checkForDuplicates"/>
        <sequenceFlow id="flow2" sourceRef="checkForDuplicates" targetRef="decisionGateway"/>
        <sequenceFlow id="flow3" sourceRef="decisionGateway" targetRef="approveCart">
            <conditionExpression xsi:type="tFormalExpression">not(isDuplicate)</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow4" sourceRef="decisionGateway" targetRef="rejectCart">
            <conditionExpression xsi:type="tFormalExpression">isDuplicate</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow5" sourceRef="approveCart" targetRef="endEvent"/>
        <sequenceFlow id="flow6" sourceRef="rejectCart" targetRef="endEvent"/>
    </process>

    <bpmndi:BPMNDiagram id="BPMNDiagram_duplicateCartWorkflow">
        <bpmndi:BPMNPlane bpmnElement="duplicateCartProcess">
            <bpmndi:BPMNShape id="startEvent_di" bpmnElement="startEvent">
                <dc:Bounds x="100" y="100" width="36" height="36"/>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape id="checkForDuplicates_di" bpmnElement="checkForDuplicates">
                <dc:Bounds x="200" y="100" width="100" height="80"/>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape id="decisionGateway_di" bpmnElement="decisionGateway">
                <dc:Bounds x="350" y="100" width="50" height="50"/>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape id="approveCart_di" bpmnElement="approveCart">
                <dc:Bounds x="450" y="50" width="100" height="80"/>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape id="rejectCart_di" bpmnElement="rejectCart">
                <dc:Bounds x="450" y="150" width="100" height="80"/>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape id="endEvent_di" bpmnElement="endEvent">
                <dc:Bounds x="600" y="100" width="36" height="36"/>
            </bpmndi:BPMNShape>

            <bpmndi:BPMNEdge id="flow1_di" bpmnElement="flow1">
                <di:waypoint x="136" y="118"/>
                <di:waypoint x="200" y="118"/>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="flow2_di" bpmnElement="flow2">
                <di:waypoint x="300" y="118"/>
                <di:waypoint x="350" y="118"/>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="flow3_di" bpmnElement="flow3">
                <di:waypoint x="400" y="118"/>
                <di:waypoint x="450" y="90"/>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="flow4_di" bpmnElement="flow4">
                <di:waypoint x="400" y="118"/>
                <di:waypoint x="450" y="150"/>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="flow5_di" bpmnElement="flow5">
                <di:waypoint x="550" y="90"/>
                <di:waypoint x="600" y="118"/>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="flow6_di" bpmnElement="flow6">
                <di:waypoint x="550" y="150"/>
                <di:waypoint x="600" y="118"/>
            </bpmndi:BPMNEdge>
        </bpmndi:BPMNPlane>
    </bpmndi:BPMNDiagram>
</definitions>
